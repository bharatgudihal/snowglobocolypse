<ATLConfig atl_name="tutorial_controls">
 <AudioTriggers>
  <ATLTrigger atl_name="Stop_town_amb_loop" path="ambience">
   <WwiseEvent wwise_name="Stop_town_amb_loop"/>
  </ATLTrigger>
  <ATLTrigger atl_name="Play_town_amb_loop" path="ambience">
   <WwiseEvent wwise_name="Play_town_amb_loop"/>
  </ATLTrigger>
  <ATLTrigger atl_name="Play_cannonball_wall_impact" path="collisions">
   <WwiseEvent wwise_name="Play_cannonball_wall_impact"/>
  </ATLTrigger>
  <ATLTrigger atl_name="Play_box_impact" path="collisions">
   <WwiseEvent wwise_name="Play_box_impact"/>
  </ATLTrigger>
  <ATLTrigger atl_name="Play_cannon_shot" path="cannon">
   <WwiseEvent wwise_name="Play_cannon_shot"/>
  </ATLTrigger>
  <ATLTrigger atl_name="Play_cannon_charge_up" path="cannon">
   <WwiseEvent wwise_name="Play_cannon_charge_up"/>
  </ATLTrigger>
  <ATLTrigger atl_name="Stop_robot_movement" path="robot">
   <WwiseEvent wwise_name="Stop_robot_movement"/>
  </ATLTrigger>
  <ATLTrigger atl_name="Play_robot_movement" path="robot">
   <WwiseEvent wwise_name="Play_robot_movement"/>
  </ATLTrigger>
  <ATLTrigger atl_name="Play_robot_land" path="robot">
   <WwiseEvent wwise_name="Play_robot_land"/>
  </ATLTrigger>
  <ATLTrigger atl_name="Play_robot_head_move" path="robot">
   <WwiseEvent wwise_name="Play_robot_head_move"/>
  </ATLTrigger>
  <ATLTrigger atl_name="Play_switch_test" path="tests">
   <WwiseEvent wwise_name="Play_switch_test"/>
  </ATLTrigger>
 </AudioTriggers>
 <AudioRTPCs>
  <ATLRtpc atl_name="AmbFade" path="ambience">
   <WwiseRtpc wwise_name="AmbFade"/>
  </ATLRtpc>
  <ATLRtpc atl_name="CannonChargeUpLevel" path="cannon">
   <WwiseRtpc wwise_name="CannonChargeUpLevel"/>
  </ATLRtpc>
  <ATLRtpc atl_name="RobotJump" path="robot">
   <WwiseRtpc wwise_name="RobotJump"/>
  </ATLRtpc>
  <ATLRtpc atl_name="RobotHead" path="robot">
   <WwiseRtpc wwise_name="RobotHead"/>
  </ATLRtpc>
  <ATLRtpc atl_name="RobotVelocity" path="robot">
   <WwiseRtpc wwise_name="RobotVelocity"/>
  </ATLRtpc>
 </AudioRTPCs>
 <AudioSwitches>
  <ATLSwitch atl_name="BeepType" path="tests">
   <ATLSwitchState atl_name="One">
    <WwiseSwitch wwise_name="BeepType">
     <WwiseValue wwise_name="One"/>
    </WwiseSwitch>
   </ATLSwitchState>
   <ATLSwitchState atl_name="Two">
    <WwiseSwitch wwise_name="BeepType">
     <WwiseValue wwise_name="Two"/>
    </WwiseSwitch>
   </ATLSwitchState>
  </ATLSwitch>
 </AudioSwitches>
 <AudioEnvironments>
  <ATLEnvironment atl_name="CathedralVerb" path="tests">
   <WwiseAuxBus wwise_name="CathedralVerb"/>
  </ATLEnvironment>
 </AudioEnvironments>
</ATLConfig>
